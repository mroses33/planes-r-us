<div class="container">

  <%= link_to "Back", root_path %>

  <div class="row">
    <div class="col-sm-12 col-md-6">
      <h3>Booking</h3>
      <table class="table table-bordered">
        <tbody>
          <thead>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Plane</th>
            <th>Paid</th>
          </thead>
          <% @booking.each do |booking| %>
            <tr>
              <td><%= booking.start_date %></td>
              <td><%= booking.end_date %></td>
              <td><%= booking.plane %></td>
              <td><%= booking.price %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
